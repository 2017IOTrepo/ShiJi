<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="DashView">
        <attr name="dashWidth" format="dimension"/>
        <attr name="lineWidth" format="dimension"/>
        <attr name="lineHeight" format="dimension"/>
        <attr name="lineColor" format="color"/>
        <attr name="dashOrientation" format="integer"/>
    </declare-styleable>

    <declare-styleable name="buttonView">
        <attr name="onBackground" format="reference"></attr>
        <attr name="offBackground" format="reference"></attr>
        <attr name="circleColor" format="reference"></attr>
        <attr name="time" format="integer"></attr>
        <attr name="layout_width" format="dimension"></attr>
        <attr name="layout_height" format="dimension"></attr>
    </declare-styleable>




</resources>